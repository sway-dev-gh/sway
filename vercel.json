{
  "functions": {
    "web/app/**/*.tsx": {
      "runtime": "@vercel/next@latest"
    }
  },
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ]
}