{
  "name": "sway-backend",
  "version": "1.0.1",
  "description": "Sway - Request files from anyone, securely",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node run-migration.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "jest --detectOpenHandles --verbose",
    "test:unit": "jest --testNamePattern=\"unit\" --testPathIgnorePatterns=\"integration|api\"",
    "test:integration": "jest --testNamePattern=\"integration\"",
    "test:api": "jest --testNamePattern=\"api\""
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csrf": "^3.1.0",
    "dotenv": "^16.3.1",
    "escape-html": "^1.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-slow-down": "^3.0.1",
    "express-validator": "^7.3.0",
    "express-winston": "^4.2.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "openai": "^6.8.1",
    "pg": "^8.11.3",
    "redis": "^5.9.0",
    "socket.io": "^4.8.1",
    "stripe": "^19.3.1",
    "validator": "^13.15.23",
    "winston": "^3.18.3",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4"
  }
}
